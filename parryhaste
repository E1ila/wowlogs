BOSS="$1"
echo Extracting boss hits
wl $1 $2 $3 $4 $5 $6 --printraw --source $BOSS --filter SWING_DAMAGE_LANDED,SWING_MISSED,SPELL_EXTRA_ATTACKS > ~/tmp/1
echo Extracting parries on boss 
wl $1 $2 $3 $4 $5 $6 --miss PARRY --printraw --target $BOSS --filter SWING_MISSED >> ~/tmp/1
sort ~/tmp/1 | wl - --print --timediff > boss_hits.txt
